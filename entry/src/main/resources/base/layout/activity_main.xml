<?xml version="1.0" encoding="utf-8"?>
<DependentLayout xmlns:ohos="http://schemas.huawei.com/res/ohos"
                 ohos:width="match_parent"
                 ohos:height="match_parent"
                 ohos:orientation="vertical"
                 ohos:background_element="#00BFFF">
    <Button
            ohos:id="$+id:shape_change"
            ohos:width="match_content"
            ohos:height="match_content"
            ohos:top_margin="30vp"
            ohos:text="ShapeChangeActivity"
            ohos:text_size="19fp"
            ohos:top_padding="8vp"
            ohos:bottom_padding="8vp"
            ohos:right_padding="80vp"
            ohos:left_padding="80vp"
            ohos:text_color="#696969"
            ohos:background_element="#FFFFFF"
            ohos:center_in_parent="true"
            ohos:align_parent_top="true"/>
    <Button
            ohos:id="$+id:goods"
            ohos:width="match_content"
            ohos:height="match_content"
            ohos:top_margin="30vp"
            ohos:text="GoodsActivity"
            ohos:text_size="19fp"
            ohos:top_padding="8vp"
            ohos:bottom_padding="8vp"
            ohos:right_padding="80vp"
            ohos:left_padding="80vp"
            ohos:text_color="#696969"
            ohos:background_element="#FFFFFF"
            ohos:center_in_parent="true"
            ohos:below="$id:shape_change"/>

    <Button
            ohos:id="$+id:animal"
            ohos:width="match_content"
            ohos:height="match_content"
            ohos:top_margin="30vp"
            ohos:text="AnimalActivity"
            ohos:text_size="19fp"
            ohos:top_padding="8vp"
            ohos:bottom_padding="8vp"
            ohos:right_padding="80vp"
            ohos:left_padding="80vp"
            ohos:text_color="#696969"
            ohos:background_element="#FFFFFF"
            ohos:center_in_parent="true"
            ohos:below="$id:goods"/>

    <Button
            ohos:id="$+id:scenery"
            ohos:width="match_content"
            ohos:height="match_content"
            ohos:top_margin="30vp"
            ohos:text="SceneryActivity"
            ohos:text_size="19fp"
            ohos:top_padding="8vp"
            ohos:bottom_padding="8vp"
            ohos:right_padding="80vp"
            ohos:left_padding="80vp"
            ohos:text_color="#696969"
            ohos:background_element="#FFFFFF"
            ohos:center_in_parent="true"
            ohos:below="$id:animal"/>

    <Button
            ohos:id="$+id:circle_rotate"
            ohos:width="match_content"
            ohos:height="match_content"
            ohos:top_margin="30vp"
            ohos:text="CircleRotateActivity"
            ohos:text_size="19fp"
            ohos:top_padding="8vp"
            ohos:bottom_padding="8vp"
            ohos:right_padding="80vp"
            ohos:left_padding="80vp"
            ohos:text_color="#696969"
            ohos:background_element="#FFFFFF"
            ohos:center_in_parent="true"
            ohos:below="$id:scenery"/>

    <Button
            ohos:id="$+id:circle_jump"
            ohos:width="match_content"
            ohos:height="match_content"
            ohos:top_margin="30vp"
            ohos:text="CircleJumpActivity"
            ohos:text_size="19fp"
            ohos:top_padding="8vp"
            ohos:bottom_padding="8vp"
            ohos:right_padding="80vp"
            ohos:left_padding="80vp"
            ohos:text_color="#696969"
            ohos:background_element="#FFFFFF"
            ohos:center_in_parent="true"
            ohos:below="$id:circle_rotate"/>
</DependentLayout>